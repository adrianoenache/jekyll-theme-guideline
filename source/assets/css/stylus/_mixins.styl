//
$reset-list()
	list-style none
	margin 0
	padding 0

//
$icon()
	display inline-block
	width 1em
	height 1em
	stroke-width 0
	stroke currentColor
	fill currentColor

//
$center()
	left 50%
	position absolute
	top 50%
	transform translate(-50%, -50%)

//
$reset-button()
	cursor pointer
	border 0
	outline none
	padding 0
